{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "semi": 1,
  
    "no-console": "warn",
    "no-debugger": "warn",
    "no-unused-vars": "warn",
    "no-undef": "warn"
  }
}